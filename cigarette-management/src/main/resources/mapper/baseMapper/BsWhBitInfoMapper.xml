<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.base.mapper.BsWhBitInfoMapper">

    <resultMap type="com.ruoyi.base.domain.BsWhBitInfo" id="BsWhBitInfoResult">
        <result property="id" column="id"/>
        <result property="whBitCoded" column="wh_bit_coded"/>
        <result property="whBitName" column="wh_bit_name"/>
        <result property="whBitCapacity" column="wh_bit_capacity"/>
        <result property="isExclusive" column="is_exclusive"/>
        <result property="exclusiveType" column="exclusive_type"/>
        <result property="whAreaCoded" column="wh_area_coded"/>
        <result property="whAreaName" column="wh_area_name"/>
        <result property="warehouseCoded" column="warehouse_coded"/>
        <result property="warehouseName" column="warehouse_name"/>
        <result property="whBitType" column="wh_bit_type"/>
        <result property="shelfCoded" column="shelf_coded"/>
        <result property="rowsNumber" column="row_number"/>
        <result property="columnNumber" column="column_number"/>
        <result property="whBitEnabled" column="wh_bit_enabled"/>
        <result property="objectCoded" column="object_coded"/>
        <result property="objectName" column="object_name"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="remark" column="remark"/>
        <result property="createTime" column="create_time"/>
        <result property="createBy" column="create_by"/>
    </resultMap>
    <update id="updateWarehouseNameByWarehouseCoded">
        update bs_wh_bit_info set warehouse_name=#{warehouseName} where  warehouse_coded=#{warehouseCoded}
    </update>
    <update id="updateWhAreaNameByWhAreaCoded">
           update bs_wh_bit_info set wh_area_name=#{whAreaName} where  wh_area_coded=#{whAreaCoded}
    </update>
    <select id="selectListByIds" resultMap="BsWhBitInfoResult">
        select * from bs_wh_bit_info
        <where>
            <if test="ids!=null and ids.size()>0">
                id in
                <foreach collection="ids" index="index" item="id" open="(" close=")" separator=",">
                    #{id}
                </foreach>
            </if>
            and wh_bit_enabled ='0'
        </where>
    </select>
</mapper>
