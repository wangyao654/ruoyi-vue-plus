<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.base.mapper.BsDealingsunitInfoMapper">

    <resultMap type="com.ruoyi.base.domain.BsDealingsunitInfo" id="BsDealingsunitInfoResult">
        <result property="id" column="id"/>
        <result property="unitCoded" column="unit_coded"/>
        <result property="unitName" column="unit_name"/>
        <result property="unitType" column="unit_type"/>
        <result property="contactPerson" column="contact_person"/>
        <result property="landlineNumber" column="landline_number"/>
        <result property="phone" column="phone"/>
        <result property="email" column="email"/>
        <result property="unitAddress" column="unit_address"/>
        <result property="unitEnabled" column="unit_enabled"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="remark" column="remark"/>
        <result property="createBy" column="create_by"/>
    </resultMap>
    <select id="selectListByIds" resultMap="BsDealingsunitInfoResult">
        select  * from bs_dealingsunit_info
        <where>
            <if test="ids!=null and ids.size()>0">
                id in
                <foreach collection="ids" close=")" separator="," open="(" index="index" item="id">
                    #{id}
                </foreach>
            </if>
            and unit_enabled = '0'
        </where>
    </select>

</mapper>
